.IFJcode20

JUMP main

LABEL foo
CREATEFRAME
DEFVAR TF@a
POPS TF@a
PUSHFRAME
DEFVAR LF@new_line
MOVE LF@new_line string@\010
WRITE LF@a
WRITE LF@new_line
POPFRAME
RETURN

LABEL bar
CREATEFRAME
DEFVAR TF@a
POPS TF@a
PUSHFRAME
DEFVAR LF@y
MOVE LF@y int@0
DEFVAR LF@bar-24
MOVE LF@bar-24 int@24
ADD LF@y LF@a LF@bar-24
POPFRAME
RETURN

LABEL main
CREATEFRAME
PUSHFRAME
DEFVAR LF@a
MOVE LF@a int@42
PUSHS LF@a
CALL foo
DEFVAR LF@b
MOVE LF@b int@0
PUSHS LF@a
CALL bar
MOVE LF@b TF@y
DEFVAR LF@new_line
MOVE LF@new_line string@@?o
WRITE LF@b
WRITE LF@new_line
DEFVAR LF@c
MOVE LF@c string@Hello\010
WRITE LF@c
POPFRAME
EXIT int@0
